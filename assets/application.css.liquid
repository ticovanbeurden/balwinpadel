/* purgecss start ignore */

/* purgecss end ignore */

.tw-mb-3 {
  margin-bottom: 0.75rem
}

.tw-flex {
  display: flex
}

.tw-hidden {
  display: none
}

.tw-h-1 {
  height: 0.25rem
}

.tw-h-full {
  height: 100%
}

.tw-w-full {
  width: 100%
}

.tw-w-2\/3 {
  width: 66.666667%
}

.tw-flex-col {
  flex-direction: column
}

.tw-gap-y-2 {
  row-gap: 0.5rem
}

.tw-bg-black\/10 {
  background-color: rgb(0 0 0 / 0.1)
}

.tw-bg-black {
  --tw-bg-opacity: 1;
  background-color: rgb(0 0 0 / var(--tw-bg-opacity))
}

.tw-text-center {
  text-align: center
}

.tw-text-\[12px\] {
  font-size: 12px
}

.tw-transition-all {
  transition-property: all;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 150ms
}

.tw-duration-300 {
  transition-duration: 300ms
}

.free-shipping .\[\.free-shipping_\&\]\:tw-block {
  display: block
}

.free-shipping .\[\.free-shipping_\&\]\:tw-hidden {
  display: none
}