/* purgecss start ignore */

/* purgecss end ignore */

.tw-relative {
  position: relative
}

.tw-z-10 {
  z-index: 10
}

.tw-m-0 {
  margin: 0px
}

.tw-mx-auto {
  margin-left: auto;
  margin-right: auto
}

.\!tw-mb-0 {
  margin-bottom: 0px !important
}

.tw-mb-0 {
  margin-bottom: 0px
}

.tw-mb-2 {
  margin-bottom: 0.5rem
}

.tw-mb-3 {
  margin-bottom: 0.75rem
}

.tw-mb-4 {
  margin-bottom: 1rem
}

.tw-mt-auto {
  margin-top: auto
}

.tw-flex {
  display: flex
}

.tw-grid {
  display: grid
}

.tw-hidden {
  display: none
}

.tw-aspect-square {
  aspect-ratio: 1 / 1
}

.tw-h-1 {
  height: 0.25rem
}

.tw-h-6 {
  height: 1.5rem
}

.tw-h-full {
  height: 100%
}

.tw-w-2\/3 {
  width: 66.666667%
}

.tw-w-full {
  width: 100%
}

.tw-cursor-pointer {
  cursor: pointer
}

.tw-flex-col {
  flex-direction: column
}

.tw-items-center {
  align-items: center
}

.tw-justify-start {
  justify-content: flex-start
}

.tw-justify-end {
  justify-content: flex-end
}

.tw-justify-center {
  justify-content: center
}

.tw-gap-2 {
  gap: 0.5rem
}

.tw-gap-y-2 {
  row-gap: 0.5rem
}

.tw-whitespace-nowrap {
  white-space: nowrap
}

.tw-border {
  border-width: 1px
}

.tw-bg-black {
  --tw-bg-opacity: 1;
  background-color: rgb(0 0 0 / var(--tw-bg-opacity))
}

.tw-bg-black\/10 {
  background-color: rgb(0 0 0 / 0.1)
}

.tw-pb-6 {
  padding-bottom: 1.5rem
}

.tw-pr-2 {
  padding-right: 0.5rem
}

.tw-pt-2 {
  padding-top: 0.5rem
}

.tw-pt-2\.5 {
  padding-top: 0.625rem
}

.tw-pt-5 {
  padding-top: 1.25rem
}

.tw-text-center {
  text-align: center
}

.tw-text-end {
  text-align: end
}

.tw-text-\[12px\] {
  font-size: 12px
}

.tw-text-xs {
  font-size: 0.75rem;
  line-height: 1rem
}

.tw-font-bold {
  font-weight: 700
}

.tw-uppercase {
  text-transform: uppercase
}

.tw-text-black {
  --tw-text-opacity: 1;
  color: rgb(0 0 0 / var(--tw-text-opacity))
}

.tw-opacity-75 {
  opacity: 0.75
}

.tw-transition-all {
  transition-property: all;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  transition-duration: 150ms
}

.tw-duration-300 {
  transition-duration: 300ms
}

.hover\:tw-bg-opacity-90:hover {
  --tw-bg-opacity: 0.9
}

@media (min-width: 1024px) {
  .lg\:tw-block {
    display: block
  }

  .lg\:tw-hidden {
    display: none
  }

  .lg\:tw-aspect-video {
    aspect-ratio: 16 / 9
  }
}

.free-shipping .\[\.free-shipping_\&\]\:tw-block {
  display: block
}

.free-shipping .\[\.free-shipping_\&\]\:tw-hidden {
  display: none
}