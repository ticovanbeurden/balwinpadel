}

.tw-text-xs {
  font-size: 0.75rem;
  line-height: 1rem
}

.tw-uppercase {
  text-transform: uppercase
}

.tw-text-white {
  --tw-text-opacity: 1;
  color: rgb(255 255 255 / var(--tw-text-opacity))
}

.tw-text-black {
  --tw-text-opacity: 1;
  color: rgb(0 0 0 / var(--tw-text-opacity))
}

